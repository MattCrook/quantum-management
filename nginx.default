server {
    listen 8000;
    server_name quantummanagement.org;

    location / {
        proxy_pass http://127.0.0.1:8020;
        proxy_set_header Host $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }
    location /static {
        root /app/quantummanagement;
    }
}
